{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "vtrunkd Control Room",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "com.vtrunkd.controlroom"
    },
    "windows": [
      {
        "title": "vtrunkd Control Room",
        "width": 1180,
        "height": 860,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
